{
  "name": "guitardeveloper.net.client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "version": "ng version",
    "update": "ng update",
    "update:cli-core": "ng update @angular/cli @angular/core --force",
    "start:prod-pl": "ng serve --configuration=prod-pl",
    "start:prod-en": "ng serve --configuration=prod-en",
    "start:debug-pl": "ng serve --configuration=debug-pl",
    "start:debug-en": "ng serve --configuration=debug-en",
    "build:prod-pl": "ng build --configuration=prod-pl",
    "build:prod-en": "ng build --configuration=prod-en",
    "build:debug-pl": "ng build --configuration=debug-pl",
    "build:debug-en": "ng build --configuration=debug-en",
    "build:ssr": "ng run GuitarDeveloper.NET.Client:server:dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n:extract": "ng xi18n --output-path locales",
    "i18n:trans-pl": "npx kendo-translate src/locales/messages.pl.xlf --locale pl-PL --force",
    "i18n:trans-en": "npx kendo-translate src/locales/messages.en.xlf --locale en-US --force",
    "i18n:extract-pl": "ng xi18n --i18n-locale pl --out-file locales/messages.pl.xlf",
    "i18n:extract-en": "ng xi18n --i18n-locale en --out-file locales/messages.en.xlf",
    "i18n": " npm run i18n-templates && npm run i18n-ts && npm run i18n-merge",
    "i18n-templates": "ng xi18n -f xlf --output-path src/locales --outFile=messages.xlf --locale pl",
    "i18n-ts": "ngx-extractor -i \"src/**/*.ts\" -f xlf -o src/locales.messages.xlf",
    "i18n-merge": "for lang in pl en; do xliffmerge --profile xliffmerge.json es $lang; done"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.12",
    "@angular/common": "8.2.12",
    "@angular/compiler": "8.2.12",
    "@angular/core": "8.2.12",
    "@angular/forms": "8.2.12",
    "@angular/platform-browser": "8.2.12",
    "@angular/platform-browser-dynamic": "8.2.12",
    "@angular/platform-server": "8.2.12",
    "@angular/router": "8.2.12",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@ngx-translate/core": "^12.1.2",
    "@popperjs/core": "^2.3.3",
    "@progress/kendo-angular-buttons": "^5.4.1",
    "@progress/kendo-angular-common": "^1.2.2",
    "@progress/kendo-angular-dateinputs": "^4.2.2",
    "@progress/kendo-angular-dialog": "^4.1.3",
    "@progress/kendo-angular-dropdowns": "^4.2.6",
    "@progress/kendo-angular-editor": "^1.0.6",
    "@progress/kendo-angular-excel-export": "^3.1.3",
    "@progress/kendo-angular-grid": "^4.7.1",
    "@progress/kendo-angular-inputs": "^6.5.1",
    "@progress/kendo-angular-intl": "^2.0.1",
    "@progress/kendo-angular-l10n": "^2.0.1",
    "@progress/kendo-angular-layout": "^4.2.3",
    "@progress/kendo-angular-messages": "^1.19.0",
    "@progress/kendo-angular-pdf-export": "^2.0.3",
    "@progress/kendo-angular-popup": "^3.0.5",
    "@progress/kendo-angular-toolbar": "^3.1.1",
    "@progress/kendo-angular-tooltip": "^2.1.2",
    "@progress/kendo-angular-upload": "^5.2.1",
    "@progress/kendo-data-query": "^1.5.3",
    "@progress/kendo-drawing": "^1.6.0",
    "@progress/kendo-theme-default": "^4.17.0",
    "angular-notifier": "^6.0.1",
    "aspnet-prerendering": "^3.0.1",
    "bootstrap": "^4.3.1",
    "core-js": "^3.3.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "localize-router": "^2.0.0-RC.3",
    "localize-router-http-loader": "^1.0.2",
    "oidc-client": "^1.9.1",
    "popper.js": "^1.16.1",
    "rxjs": "^6.5.3",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.24",
    "@angular/cli": "8.3.14",
    "@angular/compiler-cli": "8.2.12",
    "@angular/language-service": "8.2.12",
    "@types/jasmine": "~3.4.4",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~12.11.6",
    "codelyzer": "^5.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "typescript": "3.5.3"
  },
  "optionalDependencies": {
    "node-sass": "^4.12.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0"
  }
}
