{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "GuitarDeveloper.NET.Client": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "progress": false,
            "extractCss": true,
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/assets",
              "src/assets/favicon.ico"
            ],
            "styles": [
              {
                "input": "node_modules/@progress/kendo-theme-default/dist/all.css"
              },
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/styles.css",
              "node_modules/angular-notifier/styles.scss",
              "node_modules/font-awesome/css/font-awesome.css",
              "src/all.css"
            ],
            "scripts": [
              "src/jquery-3.4.1.slim.min.js",
              "src/popper.min.js",
              "src/bootstrap.min.js"
            ]
          },
          "configurations": {
            "prod-pl": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.pl.ts"
                }
              ],
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "outputPath": "dist/pl",
              "i18nFile": "src/locales/messages.pl.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "pl",
              "i18nMissingTranslation": "error"
            },
            "prod-en": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.en.ts"
                }
              ],
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "outputPath": "dist/en",
              "i18nFile": "src/locales/messages.en.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "en",
              "i18nMissingTranslation": "error"
            },
            "debug-pl": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.debug.pl.ts"
                }
              ],
              "aot": true,
              "outputPath": "dist/pl",
              "i18nFile": "src/locales/messages.pl.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "pl",
              "i18nMissingTranslation": "error"
            },
            "debug-en": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.debug.en.ts"
                }
              ],
              "aot": true,
              "outputPath": "dist/en",
              "i18nFile": "src/locales/messages.en.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "en",
              "i18nMissingTranslation": "error"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "GuitarDeveloper.NET.Client:build"
          },
          "configurations": {
            "prod-pl": {
              "browserTarget": "GuitarDeveloper.NET.Client:build:prod-pl"
            },
            "prod-en": {
              "browserTarget": "GuitarDeveloper.NET.Client:build:prod-en"
            },
            "debug-pl": {
              "browserTarget": "GuitarDeveloper.NET.Client:build:debug-pl"
            },
            "debug-en": {
              "browserTarget": "GuitarDeveloper.NET.Client:build:debug-en"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "GuitarDeveloper.NET.Client:build"
          },
          "configurations": {
            "prod-pl": {
              "browserTarget": "GuitarDeveloper.NET.Client:build:prod-pl"
            },
            "prod-en": {
              "browserTarget": "GuitarDeveloper.NET.Client:build:prod-en"
            },
            "debug-pl": {
              "browserTarget": "GuitarDeveloper.NET.Client:build:debug-pl"
            },
            "debug-en": {
              "browserTarget": "GuitarDeveloper.NET.Client:build:debug-en"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [ "src/styles.css" ],
            "scripts": [],
            "assets": [ "src/assets" ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [ "src/tsconfig.app.json", "src/tsconfig.spec.json" ],
            "exclude": [ "**/node_modules/**" ]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist-server",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.server.json"
          },
          "configurations": {
            "dev": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": true
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false
            }
          }
        }
      }
    },
    "GuitarDeveloper.NET.Client-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "GuitarDeveloper.NET.Client:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [ "**/node_modules/**" ]
          }
        }
      }
    }
  },
  "defaultProject": "GuitarDeveloper.NET.Client"
}
